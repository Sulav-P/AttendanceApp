<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".admin.ADMINFEEDBACK.FEEDBACKMAIN"
    android:background="@drawable/blue3"
    android:orientation="vertical">

    <Button
        android:id="@+id/studentfeedback"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="165dp"
        android:background="@drawable/round_button"
        android:text="STUDENT FEEDBACK"
        android:layout_alignParentLeft="true" />

    <Button
        android:id="@+id/parentfeedback"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="253dp"
        android:background="@drawable/round_button"
        android:text="PARENT FEEDBACK"
        android:layout_alignParentLeft="true" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        app:srcCompat="@drawable/he"
        android:layout_alignParentLeft="true" />

</RelativeLayout>